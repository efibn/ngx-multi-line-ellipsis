{
    "name": "ngx-multi-line-ellipsis-wrapper",
    "version": "9.0.5",
    "description": "Angular directive for multi-line ellipsis. Support Angular 9.0.1",
    "main": "index.ts",
    "scripts": {
        "transpile": "ngc",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/ngx-multi-line-ellipsis-wrapper.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-multi-line-ellipsis-wrapper.umd.min.js",
        "build": "npm run transpile && npm run package && npm run minify",
        "version": "auto-changelog -p && git add CHANGELOG.md"
    },
    "keywords": [
        "multi",
        "line",
        "ellipsis",
        "angular",
        "angular5",
        "angular9"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/efibn/ngx-multi-line-ellipsis-wrapper.git"
    },
    "author": "https://github.com/efibn",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/efibn/ngx-multi-line-ellipsis-wrapper/issues"
    },
    "homepage": "https://github.com/efibn/ngx-multi-line-ellipsis-wrapper#readme",
    "devDependencies": {
        "@angular/compiler": "^9.0.1",
        "@angular/compiler-cli": "^9.0.1",
        "@angular/core": "^9.0.1",
        "rollup": "^1.7.4",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rxjs": "^6.4.0",
        "typescript": "^3.8.3",
        "uglify-js": "^3.5.3",
        "auto-changelog": "*"
    }
}
