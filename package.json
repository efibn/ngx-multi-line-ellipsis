{
  "name": "@davidperis92/ngx-multi-line-ellipsis",
  "version": "1.0.8",
  "description": "Angular directive for multi-line ellipsis.",
  "main": "index.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-multi-line-ellipsis.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-multi-line-ellipsis.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "keywords": [
    "multi",
    "line",
    "ellipsis",
    "angular",
    "angular5"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidperis92/ngx-multi-line-ellipsis.git"
  },
  "author": "https://github.com/davidperis92",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidperis92/ngx-multi-line-ellipsis/issues"
  },
  "homepage": "https://github.com/davidperis92/ngx-multi-line-ellipsis#readme",
  "devDependencies": {
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/core": "^5.2.11",
    "rollup": "^1.7.4",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rxjs": "^6.4.0",
    "typescript": "^2.8.4",
    "uglify-js": "^3.5.3",
    "auto-changelog": "*"
  }
}
